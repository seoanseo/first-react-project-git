{
  "name": "@hubspot/cms-components",
  "version": "1.0.10",
  "license": "Apache-2.0",
  "description": "For use with JS Rendering on the CMS",
  "engines": {
    "node": ">=20.x"
  },
  "type": "module",
  "main": "dist/index.js",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "development": "./dist/index.js",
      "server": "./dist/server/index.js",
      "default": "./dist/index.js"
    },
    "./fields": {
      "types": "./types/fields/index.d.ts",
      "development": "./dist/fields/index.js",
      "server": "./dist/server/fields/index.js",
      "default": "./dist/fields/index.js"
    },
    "./modules": {
      "types": "./types/modules/index.d.ts",
      "development": "./dist/modules/index.js",
      "server": "./dist/server/modules/index.js",
      "default": "./dist/modules/index.js"
    },
    "./config": {
      "types": "./types/config/index.d.ts",
      "default": "./dist/config/index.js"
    },
    "./config-runtime": {
      "types": "./types/config/index.d.ts",
      "default": "./dist/config/index.js"
    },
    "./internal": {
      "types": "./types/internal/index.d.ts",
      "server": "./dist/server/internal/index.js"
    },
    "./islands": {
      "types": "./islands.d.ts"
    }
  },
  "scripts": {
    "clean": "rm -rf dist/",
    "build": "tsc -noEmit && yarn build-types && node scripts/build.mjs; npm run publint",
    "build:prod": "VITE_SSR=true NODE_ENV=production npm run build",
    "build:watch": "nodemon --exec 'yarn build'",
    "build-types": "tsc --project tsconfig.types.json",
    "docs:field-types": "typedoc --options typedoc.json src/fields/fieldTypes.ts",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "prepare": "npm run build",
    "publint": "publint --level warning --prod"
  },
  "dependencies": {
    "dot-prop": "^8.0.2",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "esbuild": "^0.16.17",
    "nodemon": "^2.0.15",
    "publint": "^0.2.7",
    "typedoc": "^0.25.0",
    "typedoc-plugin-markdown": "^4.0.0-next.19",
    "typedoc-vitepress-theme": "^1.0.0-next.3",
    "typescript": "~4.7.0",
    "vite": "5.4.8",
    "vitest": "1.6.0"
  },
  "files": [
    "dist/**",
    "types/**",
    "islands.d.ts"
  ],
  "build:watch": "nodemon --exec 'yarn build'",
  "nodemonConfig": {
    "ext": "js,json,ts,tsx,jsx",
    "ignore": [
      "dist/*"
    ],
    "watch": [
      "src",
      "package.json",
      "yarn.lock",
      "tsconfig.json"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "95335ef8cf0d3f3892dddc0a5e480859e632eaf4"
}
